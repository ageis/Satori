{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Satori",
  "description": "Tamper-resistant Tor Browser Bundle downloads",
  "version": "0.1",
  "manifest_version": 2,
  "offline_enabled": true,
  "minimum_chrome_version": "19",
  "app": {
    "background": {
      "scripts": [
        "sparkle.js"
      ]
    }
  },
  "icons": {
     "24": "images/24.png",
     "48": "images/48.png",
     "128": "images/128.png"
  },
  "permissions": [
    "fileSystem",
    "clipboardWrite",
    "storage"
  ]
}
